@article{miton_forward_2020,
	title = {A {Forward} {Bias} in {Human} {Profile}-{Oriented} {Portraits}},
	volume = {44},
	issn = {1551-6709},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/cogs.12866},
	doi = {10.1111/cogs.12866},
	abstract = {The spatial composition of human portraits obeys historically changing cultural norms. We show that it is also affected by cognitive factors that cause greater spontaneous attention to what is in front rather in the back of an agent. Scenes with more space in front of a directed object are both more often produced and judged as more aesthetically pleasant. This leads to the prediction that, in profile-oriented human portraits, compositions with more space in front of depicted agents (a “forward bias”) should be over-represented. By analyzing a large dataset (total N of 1,831 paintings by 582 unique identified European painters from the 15th to the 20th century), we found evidence of this forward bias: Painters tended to put more free space in front of, rather than behind, the sitters. Additionally, we found evidence that this forward bias became stronger when cultural norms of spatial composition favoring centering became less stringent.},
	language = {en},
	number = {6},
	urldate = {2021-10-06},
	journal = {Cognitive Science},
	author = {Miton, Helena and Sperber, Dan and Hernik, Mikołaj},
	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/cogs.12866},
	keywords = {read, Anteroposterior body structure, Cultural attraction, Cultural evolution, Historical norms, Portraits, Spatial composition, Visual art, WikiArt},
	pages = {e12866},
	file = {Full Text PDF:/Users/kuchbos/Zotero/storage/QI3W7FKD/Miton et al. - 2020 - A Forward Bias in Human Profile-Oriented Portraits.pdf:application/pdf;Snapshot:/Users/kuchbos/Zotero/storage/EYZDQJ96/cogs.html:text/html},
